<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="com.example.pollo.madtowncompetitionapp2018">

    <uses-permission android:name="android.permission.INTERNET" />
    <uses-permission android:name="android.permission.CAMERA" />
    <uses-permission android:name="android.permission.READ_EXTERNAL_STORAGE" />
    <uses-permission android:name="android.permission.WRITE_EXTERNAL_STORAGE" />

    <application
        android:allowBackup="true"
        android:icon="@mipmap/ic_launcher"
        android:label="@string/app_name"
        android:roundIcon="@mipmap/ic_launcher_round"
        android:supportsRtl="true"
        android:theme="@style/AppTheme">
        <activity android:name=".AppMenu">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
        <activity android:name=".ScouterInfo" />
        <activity android:name=".ScoutingMenu" />
        <activity
            android:name=".TabbedScouting"
            android:label="@string/title_activity_tabbed_scouting"
            android:theme="@style/AppTheme.NoActionBar" />
        <activity android:name=".UploadData" />
        <activity android:name=".DataListAdapter" />
        <activity android:name=".EditData" />
        <activity android:name=".TeamRoster" />
        <activity android:name=".TeamListAdapter"></activity>
    </application>

</manifest>